{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "Opens up a local webpage",
    "browser_action": {
    "default_icon": "icon.png",
    "popup": "index.html"
  },
  "default_icon": "icon.png",
    "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }],
  "permissions": [
    "tabs"
  ]
}
 